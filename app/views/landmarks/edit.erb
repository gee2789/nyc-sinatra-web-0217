<h1>Edit <%= @landmark.name %></h1>

<form action="/landmarks/<%=@landmark.id%>" method="post">
<input id="hidden" type="hidden" name="_method" value="patch">

<h3><label>Edit Your Landmark</label></h3>
<label>Landmark</label>
  <input type="text" name="landmark[name]" id="name" value="<%=@landmark.name%>"/> </label></h4>
<label>Year Completed</label>
    <input type="text" name="landmark[year_completed]" id="year_completed" value="<%=@landmark.year_completed%>"/> </label></h4>
<br />

<input type="submit" id="Edit Landmark">
</form>
